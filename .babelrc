{
  "sourceMap": true,
  "presets": [
    ["@babel/preset-env", {
      "useBuiltIns": "usage",
      "targets": {
        "browsers": ["last 2 versions", "safari > 9", "ie 11"]
      },
      "shippedProposals": true,
    }],
    "@babel/preset-react",
    ["@babel/preset-stage-0", {
      "decoratorsLegacy": true,
      "loose": true,
    }],
  ],
  "plugins": [
    ["babel-plugin-styled-components"],
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel",
      ]
    },
    "production": {
    },
  }
}
